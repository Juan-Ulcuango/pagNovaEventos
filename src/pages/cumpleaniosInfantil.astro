---
import Layout from '../layouts/Layout.astro';
import Navbar from '../components/Navbar.astro';
import CardECommerce1 from '../components/CardECommerce1.astro';
import CardECommerce2 from '../components/CardECommerce2.astro';
import Footer from '../components/Footer.astro';
---
<Layout title="Nova Eventos - Cumpleaños Infantil">
    <div id="cumpleaniosInfantil" class="pt-20">
        <Navbar></Navbar>
        <CardECommerce1 title='Pieza Central para Decoración de Eventos' description='Diseñado para eventos como cumpleaños, bautizos, graduaciones y muchos más, combina elegancia y comodidad. Fabricado con materiales resistentes y fácil de transportar, garantiza un toque sofisticado en cualquier decoración.' uniqueId="cumpleaniosInfantil1" images={['/cumpleaniosInfantil/cumpleInfantil1.webp', '/cumpleaniosInfantil/cumpleInfantil2.webp']}></CardECommerce1>
        <CardECommerce2 title='Pieza Central para Decoración de Eventos' description='Diseñado para eventos como cumpleaños, bautizos, graduaciones y muchos más, combina elegancia y comodidad. Fabricado con materiales resistentes y fácil de transportar, garantiza un toque sofisticado en cualquier decoración.' uniqueId="cumpleaniosInfantil2" images={['/cumpleaniosInfantil/cumpleInfantil3.webp', '/cumpleaniosInfantil/cumpleInfantil4.webp']}></CardECommerce2>
        <CardECommerce1 title='Pieza Central para Decoración de Eventos' description='Diseñado para eventos como cumpleaños, bautizos, graduaciones y muchos más, combina elegancia y comodidad. Fabricado con materiales resistentes y fácil de transportar, garantiza un toque sofisticado en cualquier decoración.' uniqueId="cumpleaniosInfantil3" images={['/cumpleaniosInfantil/cumpleInfantil5.webp', '/cumpleaniosInfantil/cumpleInfantil6.webp']}></CardECommerce1>
        <Footer></Footer>
    </div>
</Layout>