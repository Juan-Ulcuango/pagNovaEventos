---
import Layout from '../layouts/Layout.astro';
import Navbar from '../components/Navbar.astro';
import CardECommerce1 from '../components/CardECommerce1.astro';
import CardECommerce2 from '../components/CardECommerce2.astro';
import Footer from '../components/Footer.astro';

---
<Layout title="Nova Eventos - Baby Shower">
    <div id="babyShower" class="pt-20">
        <Navbar></Navbar>
        <CardECommerce1 title='Pieza Central para Decoración de Eventos' description='Diseñado para eventos como cumpleaños, bautizos, graduaciones y muchos más, combina elegancia y comodidad. Fabricado con materiales resistentes y fácil de transportar, garantiza un toque sofisticado en cualquier decoración.' uniqueId="babyShower1" images={['/babyShowers/babyShower1.webp', '/babyShowers/babyShower2.webp','/babyShowers/babyShower3.webp']}></CardECommerce1>
        <CardECommerce2 title='Pieza Central para Decoración de Eventos' description='Diseñado para eventos como cumpleaños, bautizos, graduaciones y muchos más, combina elegancia y comodidad. Fabricado con materiales resistentes y fácil de transportar, garantiza un toque sofisticado en cualquier decoración.' uniqueId="babyShower2" images={['/babyShowers/babyShower4.webp', '/babyShowers/babyShower5.webp','/babyShowers/babyShower6.webp']}></CardECommerce2>
        <CardECommerce1 title='Pieza Central para Decoración de Eventos' description='Diseñado para eventos como cumpleaños, bautizos, graduaciones y muchos más, combina elegancia y comodidad. Fabricado con materiales resistentes y fácil de transportar, garantiza un toque sofisticado en cualquier decoración.' uniqueId="babyShower3" images={['/babyShowers/babyShower1.webp', '/babyShowers/babyShower2.webp','/babyShowers/babyShower3.webp']}></CardECommerce1>
        <Footer></Footer>
    </div>
</Layout>