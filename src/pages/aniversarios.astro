---
import Layout from '../layouts/Layout.astro';
import Navbar from '../components/Navbar.astro';
import CardECommerce1 from '../components/CardECommerce1.astro';
import CardECommerce2 from '../components/CardECommerce2.astro';
import Footer from '../components/Footer.astro';
---
<Layout title="Nova Eventos - Aniversarios">
    <div id="aniversarios" class="pt-20">
        <Navbar></Navbar>
        <CardECommerce1 title='Pieza Central para Decoración de Eventos' description='Diseñado para eventos como cumpleaños, bautizos, graduaciones y muchos más, combina elegancia y comodidad. Fabricado con materiales resistentes y fácil de transportar, garantiza un toque sofisticado en cualquier decoración.' uniqueId="aniversarios1" images={['/aniversarios/aniversario1.webp', '/aniversarios/aniversario2.webp']}></CardECommerce1>
        <CardECommerce2 title='Pieza Central para Decoración de Eventos' description='Diseñado para eventos como cumpleaños, bautizos, graduaciones y muchos más, combina elegancia y comodidad. Fabricado con materiales resistentes y fácil de transportar, garantiza un toque sofisticado en cualquier decoración.' uniqueId="aniversarios2" images={['/aniversarios/aniversario1.webp', '/aniversarios/aniversario2.webp']}></CardECommerce2>
        <CardECommerce1 title='Pieza Central para Decoración de Eventos' description='Diseñado para eventos como cumpleaños, bautizos, graduaciones y muchos más, combina elegancia y comodidad. Fabricado con materiales resistentes y fácil de transportar, garantiza un toque sofisticado en cualquier decoración.' uniqueId="aniversarios3" images={['/aniversarios/aniversario1.webp', '/aniversarios/aniversario2.webp']}></CardECommerce1>
        <Footer></Footer>
    </div>
</Layout>